{
  "name": "ai-vite-plugins",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build:packages": "turbo run build",
    "dev:packages": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "docs:dev": "pnpm --filter ai-vite-plugins-docs docs:dev",
    "docs:build": "pnpm --filter ai-vite-plugins-docs docs:build",
    "docs:preview": "pnpm --filter ai-vite-plugins-docs docs:preview",
    "changeset": "changeset",
    "version": "changeset version",
    "publish": "changeset publish"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/openai": "^0.3.0",
    "picocolors": "^1.1.1",
    "vue": "^3.4.0",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.0",
    "dotenv": "^16.3.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "turbo": "^1.13.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-checker": "^0.6.0",
    "vue-tsc": "^1.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
